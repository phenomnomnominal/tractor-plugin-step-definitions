{
  "name": "tractor-plugin-cucumber-step-definitions",
  "version": "0.1.0",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-cucumber-step-definitions.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-cucumber-step-definitions/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/server/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile && npm run bundle",
    "bundle": "webpack --config webpack.config.js",
    "compile": "babel src/server/ --out-dir dist/server/ --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/server/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.5",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.2.0",
    "html-loader": "^0.5.1",
    "istanbul": "1.0.0-alpha.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "ng-annotate-loader": "^0.6.1",
    "npm-watch": "^0.2.0",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "change-case": "^3.0.1",
    "esquery": "^1.0.0",
    "graceful-fs": "^4.1.11",
    "tractor-error-handler": "^0.2.1",
    "tractor-file-javascript": "^0.1.1",
    "tractor-logger": "^0.1.0"
  }
}
